{
  "name": "esy-ocaml-project",
  "version": "0.1.0",
  "description": "OCaml workflow with esy",
  "license": "MIT",
  "pjc": {
    "build": "make -f $solvuu_build__install/lib/solvuu-build/solvuu.mk .merlin byte native _build/META && rm -rf $cur__lib/$cur__name && ocamlfind install $cur__name $cur__target_dir/META"
  },
  "scripts": {
    "build": "esy build",
    "shell": "esy shell",
    "clean": "esy clean",
    "ocaml": "esy ocaml",
    "editor": "esy $EDITOR"
  },
  "dependencies": {
    "solvuu-build": "andreypopp/solvuu-build#pjc",
    "merlin": "andreypopp/merlin#esy"
  },
  "peerDependencies": {
    "ocaml": "andreypopp/ocaml#esy"
  },
  "devDependencies": {
    "esy": "andreypopp/esy#8a0181b12c9d53fda95b0f9e755bba57148e2347",
    "ocaml": "andreypopp/ocaml#esy"
  }
}
