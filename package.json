{
  "name": "esy-ocaml-project",
  "version": "0.1.0",
  "description": "OCaml workflow with esy",
  "license": "MIT",
  "pjc": {
    "build": "make -f $solvuu_build__install/lib/solvuu-build/solvuu.mk byte _build/META && rm -rf $cur__lib/$cur__name && ocamlfind install $cur__name $cur__target_dir/META"
  },
  "scripts": {
    "build": "esy build",
    "shell": "esy shell",
    "clean": "esy clean",
    "ocaml": "esy ocaml"
  },
  "dependencies": {
    "ocaml": "andreypopp/ocaml#trunk",
    "solvuu-build": "andreypopp/solvuu-build#pjc"
  },
  "devDependencies": {
    "esy": "andreypopp/esy#714702f8fc8524b3a87dbff114714f71a7976286"
  }
}
