{
  "name": "esy-ocaml-project",
  "version": "0.1.0",
  "description": "OCaml workflow with esy",
  "license": "MIT",
  "esy": {
    "build": [
      "make -f $solvuu_build__install/lib/solvuu-build/solvuu.mk .merlin byte native _build/META",
      "rm -rf $cur__lib/$cur__name",
      "ocamlfind install $cur__name $cur__target_dir/META"
    ]
  },
  "scripts": {
    "build": "esy build",
    "shell": "esy shell",
    "clean": "esy clean",
    "ocaml": "esy ocaml",
    "editor": "esy $EDITOR"
  },
  "dependencies": {
    "solvuu-build": "andreypopp/solvuu-build#pjc",
    "merlin": "andreypopp/merlin#esy"
  },
  "peerDependencies": {
    "ocaml": "andreypopp/ocaml#esy"
  },
  "devDependencies": {
    "esy": "andreypopp/esy#e89de8e",
    "ocaml": "andreypopp/ocaml#esy"
  }
}
