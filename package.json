{
  "name": "esy-ocaml-project",
  "version": "0.1.0",
  "description": "OCaml workflow with esy",
  "license": "MIT",
  "esy": {
    "build": [
      "make -f $opam_solvuu_build__install/lib/solvuu-build/solvuu.mk .merlin .ocamlinit byte native _build/META",
      "rm -rf $cur__lib/$cur__name",
      "ocamlfind install $cur__name $cur__target_dir/META"
    ]
  },
  "scripts": {
    "build": "esy build",
    "shell": "esy shell",
    "clean": "esy clean",
    "repl": "esy utop",
    "editor": "esy $EDITOR"
  },
  "dependencies": {
    "@opam/solvuu-build": "*",
    "@opam/merlin": "*",
    "@opam/utop": "*"
  },
  "peerDependencies": {
    "ocaml": "andreypopp/ocaml#esy"
  },
  "devDependencies": {
    "esy": "andreypopp/esy#8fa320c",
    "ocaml": "andreypopp/ocaml#esy"
  }
}
